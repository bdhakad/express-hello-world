echo "[all]" > inventory
aws ec2 describe-instances \
   --query 'Reservations[*].Instances[*].PublicIpAddress' \
   --output text >> inventory


   or

aws ec2 describe-instances \
\
        --query 'Reservations[*].Instances[*].PublicIpAddress' \
      --filters "Name=tag:Name,Values=udacity" \
      --output text   >> inventory


if curl -s --head "https://google333.com"
then 
   echo "it worked"
else
   echo "it failed"
fi          

aws --region=us-east-1 ssm get-parameter --name "PublicIp" --with-decryption --output text --query Parameter.Value
fi          

aws --region=us-east-1 ssm get-parameter --name "PublicIp" --with-decryption --output text --query Parameter.Value


aws cloudformation deploy --template-file cloudfront.yml --stack-name production-distro --parameter-overrides PipelineID=mybucket644752792305 --tags project=udapeople

distributionId=${aws cloudformation describe-stacks --stack-name production-distro --query "Stacks[0].Outputs[?OutputKey=='WebpageCDN'].OutputValue" --output text}
echo distributionId